@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Flash Cards</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/toastr.css" rel="stylesheet" />
    <link href='http://fonts.googleapis.com/css?family=Amatic+SC:400,700' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Covered+By+Your+Grace' rel='stylesheet' type='text/css'>
    <style>
        body {
            background-color: rgb(240, 240, 240);
        }

        .small-card, .question {
            background-image: url('data:image/gif;base64,') /*/content/images/indexcard.gif*/;
            -ms-border-radius: 1px;
            border-radius: 1px;
            -webkit-transform: rotate(-1deg);
            -moz-transform: rotate(-1deg);
            -ms-transform: rotate(-1deg);
            -o-transform: rotate(-1deg);
            transform: rotate(-1deg);
            -webkit-box-shadow: 7px 7px 17px 0px #323232;
            -webkit-box-shadow: 7px 7px 17px 0px rgba(50, 50, 50, 0.75);
            -moz-box-shadow: 7px 7px 17px 0px #323232;
            -moz-box-shadow: 7px 7px 17px 0px rgba(50, 50, 50, 0.75);
            box-shadow: 7px 7px 17px 0px rgba(50, 50, 50, 0.75);
        }

        .question {
            position: relative;
            height: 20em;
            font-weight: bold;
            font-family: 'Covered By Your Grace', cursive;
            color: rgb(80, 80, 80);
            padding: 20px;
            width: 62%;
            -webkit-transform: rotate(0deg);
            -moz-transform: rotate(0deg);
            -ms-transform: rotate(0deg);
            -o-transform: rotate(0deg);
            transform: rotate(0deg);
        }

            .question canvas {
                position: absolute;
                right: -44px;
                bottom: 10px;
            }

        .q-buttons {
            position: absolute;
            bottom: 10px;
            right: 20px;
        }

            .q-buttons input {
                font-size: 2em;
            }

        .question h1, .question h2 {
            font-family: 'Covered By Your Grace', cursive;
            -webkit-transform: rotate(3deg);
            -moz-transform: rotate(3deg);
            -ms-transform: rotate(3deg);
            -o-transform: rotate(3deg);
            transform: rotate(3deg);
        }

        .small-card h2 {
            color: red;
        }

        .page-header h1 {
            font-weight: bold;
            font-family: 'Covered By Your Grace', cursive;
            font-size: 6em;
        }

        .page-header {
            color: red;
        }

            .page-header h1 > small {
                color: black;
                font-size: .7em;
            }



        li.small-card h2 {
            font-weight: bold;
            font-family: 'Covered By Your Grace', cursive;
        }
    </style>
</head>
    <body ng-app="flashCardApp">
        <script type="text/ng-template" id="/jsapps/flashcards/templates/create.html">
        <div class="row">
            <div class="col-md-6 col-md-offset-3">

                <header>
                    <h2>Create New Deck</h2>
                </header>
                <form role="form">
                    <div class="form-group">
                        <label for="title">Title</label>
                        <input type="text" ng-model="newDeck.title" ng-change="makeSlug(newDeck.title)" class="form-control" id="title" placeholder="Title For Your Deck">
                    </div>
                    <div class="form-group">
                        <label>Slug</label>
                        <span>{{newDeck.slug}}</span>
                    </div>
                    <div class="form-group">
                        <label for="question">Question</label>
                        <input type="text" ng-model="newQuestion" " class="form-control"  placeholder="Question.">
                    </div>
                    <div class="form-group">
                        <label for="answer">Answer</label>
                        <input type="text" ng-model="newAnswer" class="form-control"  placeholder="The right answer.">
                    </div>
                    <div>
                        <input type="button" value="Add Card To Deck" ng-click="addCard(newQuestion, newAnswer)" class="btn btn-info"/>
                    </div>
                </form>
                <ul class="list-unstyled">
                    <li ng-repeat="question in newDeck.cards">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                {{question.q}}<br />
                            </div>
                            <div class="panel-body">
                                {{question.a}}
                            </div>
                        </div>
                    </li>
                </ul>
                <div>
                    <input ng-disabled="notReady()" type="button" class="btn btn-lg btn-success" value="Save This Deck!" ng-click="save(newDeck)" />
                    <input type="button" class="btn btn-lg btn-danger" value="Nevermind. Go back." ng-click="cancel()"/>
                </div>
            </div>
        </div>

    </script>



        <div class="container">
            <header class="page-header">
                <h1>Flash Cards<small> Learn All Sorts of Good Stuff!</small></h1>
            </header>
            <nav>
                <ul class="nav nav-pills">
                    <li class="active"><a href="#/stacks">Study</a></li>
                    <li><a href="#">Stats</a></li>
                    <li><a href="#/create">Create</a></li>
                </ul>
            </nav>
            <div ui-view></div>

        </div>
        <script src="~/Scripts/angular.js"></script>
        <script src="~/Scripts/angular-ui-router.min.js"></script>
        <script src="~/Scripts/underscore.js"></script>
        <script src="~/additional/chart.js"></script>

        <script>
            var app = angular.module("flashCardApp", ['ui.router'])
                .config(function ($stateProvider, $urlRouterProvider) {

                    $urlRouterProvider.otherwise('/stacks');

                    $stateProvider.state('create', {
                        url: '/create',
                        templateUrl: "/jsapps/flashcards/templates/create.html",
                        controller: function ($scope, slugService, $state, flashCardPersistence) {
                            $scope.makeSlug = function (title) {
                                $scope.newDeck.slug = slugService.slugify(title);
                            };

                            $scope.newDeck = {};
                            $scope.newDeck.cards = [];

                            $scope.addCard = function (q, a) {
                                $scope.newDeck.cards.push({ q: q, a: a });
                                $scope.newQuestion= "";
                                $scope.newAnswer = "";
                                console.log($scope.newDeck);
                            };

                            $scope.notReady = function() {
                                return !($scope.newDeck.cards.length > 0 && $scope.newDeck.title != "");
                            };
                            $scope.save = function(deck) {
                                flashCardPersistence.open().then(function() {
                                    flashCardPersistence.addItem(deck).then(function() {
                                        $state.go('stacks');
                                    });
                                });
                            };
                            $scope.cancel = function() {
                                $state.go('stacks');
                            };
                        }
                    });


                    $stateProvider.state('stacks', {
                        url: '/stacks',
                        controller: "FlashCardController",
                        templateUrl: "/jsapps/flashcards/templates/stacks.html"
                    });


                    $stateProvider.state('stacks.quiz', {
                            "abstract": true,
                            url: '/quiz/:slug',
                            resolve: {
                                currentStack: function (flashCardPersistence, $stateParams, $q) {
                                    var slug = $stateParams.slug;
                                    var d = $q.defer();
                                    return flashCardPersistence.open().then(function () {
                                        flashCardPersistence.getBySlug(slug).then(function (cs) {
                                            d.resolve(cs);
                                        });
                                        return d.promise;
                                    });
                                }
                            },
                            template: "<div ui-view></div>"
                        })
                        .state("stacks.quiz.start", {
                            url: '/start',
                            controller: function ($scope, currentStack) {
                                console.log(currentStack);
                                $scope.data.currentStack = currentStack;
                            },
                            templateUrl: "/jsapps/flashcards/templates/stacks.quiz.start.html"
                        })
                        .state('stacks.quiz.summary', {
                            url: '/summary',
                            templateUrl: '/jsapps/flashcards/templates/stacks.quiz.summary.html',
                            controller: function ($scope, $state) {
                                $scope.share = function (share) {
                                    window.connection.send("guess what, NAME just got " + share.rightAnswers );
                                };
                                if (!$scope.showingSummary) {
                                    $state.go('stacks');
                                }
                            }
                        }).state("stacks.quiz.begin", {
                            url: "/:id",
                            controller: "QuizController",
                            templateUrl: "/jsapps/flashcards/templates/stacks.quiz.begin.html"
                        });
                });
        </script>
        <script src="~/jsapps/flashcards/controllers/QuizController.js"></script>
        <script src="~/jsapps/flashcards/controllers/FlashCardController.js"></script>
        <script src="~/jsapps/flashcards/services/slugService.js"></script>
        <script src="~/jsapps/flashcards/services/flashCardPersistence.js"></script>
        <script src="~/Scripts/jquery-1.9.1.js"></script>
        <script src="~/Scripts/jquery.signalR-1.2.2.js"></script>
        <script src="~/Scripts/toastr.js"></script>
        <script>
            window.connection = "";
            $(function() {
                connection = $.connection('/stats');

                connection.received(function(data) {
                    toastr.info(data);
                });

                connection.start().done(function() {
                    toastr.info("Connection Is ready");
                });
            });
        </script>
    </body>

</html>
